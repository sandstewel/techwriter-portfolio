<script type="module" src="https://unpkg.com/@google/model-viewer/dist/model-viewer.min.js"></script>

<model-viewer
  id="reveal"
  loading="eager"
  camera-controls
  touch-action="pan-y"
  auto-rotate
  poster="/assets/poster-shishkebab.webp"
  tone-mapping="aces"
  src="/models/shishkebab.glb"
  shadow-intensity="1"
  alt="A 3D model of a shishkebab">
</model-viewer>
